COLUMN TABLE "Tech.Logs" ( 
		"ID" INTEGER CS_INT GENERATED BY DEFAULT AS IDENTITY (NO CYCLE NO CACHE NO MINVALUE START WITH 000000001 INCREMENT BY 1 MAXVALUE 2999999999) NOT NULL COMMENT 'ID',
		"User" NVARCHAR(30),
		"Action" NVARCHAR(5000),
		"Errors" NVARCHAR(5000),
		"Timestamp" TIMESTAMP,
     PRIMARY KEY ("ID")
)